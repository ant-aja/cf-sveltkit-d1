{
  "name": "cf-svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite dev --host",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "cert": "rm -rf .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem localhost 127.0.0.1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-09-25 --proxy 5173 -- npm run dev",
    "pages:deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@playwright/test": "^1.38.0",
    "@sveltejs/adapter-cloudflare": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-svelte": "^2.33.1",
    "miniflare": "^3.20230922.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.10.1"
  },
  "type": "module"
}